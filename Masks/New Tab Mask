javascript:(function() {    var link = document.querySelector("link[rel*='icon']") || document.createElement('link');    link.type = 'image/x-icon';    link.rel = 'shortcut icon';    link.href = 'https://www.nicepng.com/png/detail/24-241935_google-chromium-comments-chrome-icon-png-black.png';    document.title='New Tab';    console.log(document.title);    document.getElementsByTagName('head')[0].appendChild(link);})();
