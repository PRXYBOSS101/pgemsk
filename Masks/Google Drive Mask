javascript:(function() {    var link = document.querySelector("link[rel*='icon']") || document.createElement('link');    link.type = 'image/x-icon';    link.rel = 'shortcut icon';    link.href = 'https://ssl.gstatic.com/images/branding/product/2x/hh_drive_96dp.png';    document.title='My Drive - Google Drive';    console.log(document.title);    document.getElementsByTagName('head')[0].appendChild(link);})();
