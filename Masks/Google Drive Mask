javascript:(function() {    var link = document.querySelector("link[rel*='icon']") || document.createElement('link');    link.type = 'image/x-icon';    link.rel = 'shortcut icon';    link.href = 'https://upload.wikimedia.org/wikipedia/commons/d/da/Google_Drive_logo.png';    document.title=%27My Drive - Google Drive%27;    console.log(document.title);    document.getElementsByTagName(%27head%27)[0].appendChild(link);})();
